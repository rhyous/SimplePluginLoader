        <dependency id="Autofac" version="4.9.4" />
        <dependency id="Rhyous.SimplePluginLoader" version="1.3.6" />
      </group>
      <group targetFramework="net45">
        <dependency id="Autofac" version="4.9.4" />
        <dependency id="Rhyous.SimplePluginLoader" version="1.3.6" />
      </group>
    </dependencies>
  </metadata>
  <files>
    <file src="lib\" target="lib" />
    <file src="tools\" target="tools" />
